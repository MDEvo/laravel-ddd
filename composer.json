{
    "name": "oleglfed/laravel-ddd",
    "description": "Generates domains for laravel Domain Driven Development",
    "keywords": [
        "DDD",
        "Laravel"
    ],
    "homepage": "https://github.com/oleglfed/LaravelDDD",
    "authors": [
        {
            "name": "Oleg Fedoliak",
            "email": "oleg.fedoliak@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0 || ~5.0",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "oleglfed\\LaravelDDD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "oleglfed\\LaravelDDD\\Tests\\": "tests/"
        }
    }
}
